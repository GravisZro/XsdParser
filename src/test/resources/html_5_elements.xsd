				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="controls" vs:standalone="true">
				<xsd:simpleType>
					<xsd:restriction base="xsd:NMTOKEN">
						<xsd:enumeration value="controls" />
